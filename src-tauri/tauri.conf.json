{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "customer-service-daemon",
  "version": "0.1.1",
  "identifier": "go.ke.nairobicitycounty-daemon",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Customer Service Agent",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQzQTZFNTcxNjJEODJBOUIKUldTYkt0aGljZVdtMHhpeWZpcFVPbWR0aTQwQ2NQeDR6TmN1eXRPWWhZWVBjOFluZUpGRGxQbmIK",
      "endpoints": [
        "http://customerserviceapp.nairobi.go.ke/updates/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
